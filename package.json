{
  "name": "mtt-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "f:prettier": "prettier --write .",
    "f:eslint": "eslint --fix ."
